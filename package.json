{
  "name": "ts-lxd",
  "version": "0.0.1-beta.0",
  "description": "A TypeScript client for LXD container virtualization, ported from node-lxd",
  "main": "lib/index.js",
  "keywords": [
    "lxd",
    "lxc",
    "container",
    "virtualization",
    "node-lxd",
    "cgroups",
    "os",
    "linux",
    "simple",
    "sandbox",
    "isolation"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "tslint -c tslint.json \"(src|test)/**/*.ts\""
  },
  "devDependencies": {
    "@types/debug": "^4.1.2",
    "@types/node": "^11.11.7",
    "@types/ws": "^6.0.1",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "tslint": "^5.13.1",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "debug": "^4.1.1",
    "websocket-stream": "^5.3.0",
    "ws": "^6.2.0"
  },
  "author": {
    "name": "Ben Burns",
    "email": "ben@trufflesuite.com",
    "url": "https://truffleframework.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/trufflesuite/ts-lxd.git"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "packages/*/+(src|test)/**/*.ts": [
      "tslint -c tslint.json --fix",
      "git add"
    ]
  }
}
